    "description": "Clockify to Mission Timecard",
    "type": "library",
    "require": {
        "php": "^8.0",
        "phpunit/phpunit": "^9.5",
        "vlucas/phpdotenv": "^5.4",
        "guzzlehttp/guzzle": "^7.4"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.6"
    },
    "license": "MIT",
    "autoload": {
