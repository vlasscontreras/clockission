            "VlassContreras\\Clockission\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Vlass Contreras",
            "email": "17748144+vlasscontreras@users.noreply.github.com"
        }
    ],
